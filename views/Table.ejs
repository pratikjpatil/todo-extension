<style>
    #itemlist{
        font-family:monospace;
        
    }
</style>
<h2 id="itemlist" class="text-center">List & Items Database</h2>
<table class=" container table ">
    <tr>
        <th>Items</th>
        <th>Time1</th>
        <th>Time2</th>
    </tr>
    <%  mainLists.forEach(function(list){ %>
        <th class="table-secondary"><%= list.name %></th>
        <%  list.items.forEach(function(item){ %>
            
            <tr>
                
                <td><%= item.name %></td>
                <td><%= item.time %></td>
                <td><%= item.time2 %></td>
            </tr>
            
        <% }) %>
       
    <% }) %>
</table>


